package clases;

public class Cliente extends Persona 
{
	private String tipo;

	/**
	 * 
	 */
	public Cliente() 
	{
	}

	/**
	 * @param tipo
	 */
	public Cliente(String tipo) 
	{
		this.tipo = tipo;
	}

	public String getTipo() 
	{
		return tipo;
	}

	public void setTipo(String tipo) 
	{
		this.tipo = tipo;
	}
	
	
	

}
