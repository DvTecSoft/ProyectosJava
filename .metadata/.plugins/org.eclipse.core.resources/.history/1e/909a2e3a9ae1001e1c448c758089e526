package clases;

public class Rectangulo extends Figura 
{
	
	float base; 
	float altura;
	
	/**
	 * 
	 */
	public Rectangulo() 
	{
	} 

	public Rectangulo(int base, int altura) 
	{
		this.base = base;
		this.altura = altura;
	}

	
	@Override
	public float area() 
	{
		return base * altura;
	}




	

	
}
