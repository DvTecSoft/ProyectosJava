package clases;

public class Animal 
{

	protected String nombre; 
	protected double peso;
	/**
	 * 
	 */
	public Animal() {
	}
	/**
	 * @param nombre
	 * @param peso
	 */
	public Animal(String nombre, double peso) {
		this.nombre = nombre;
		this.peso = peso;
	}
	public String getNombre() {
		return nombre;
	}
	public void setNombre(String nombre) {
		this.nombre = nombre;
	}
	public double getPeso() {
		return peso;
	}
	public void setPeso(double peso) {
		this.peso = peso;
	}
	
	
	
}
